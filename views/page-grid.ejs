<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('partials/header'); %>
    <title>Journel</title>
  </head>
  <body>
    <%- include("partials/navbar"); %>
    <main>
      <div class="<%-pageName %> mt-xs <%- pageClasses.join(' ') %>">
        <div
          class="page__heading <%-pageName %>__heading <%- pageHeadingClasses %>"
        >
          <%= heading %>
        </div>
        <div
          class="<%-pageName %>__main mt-sm <%- pageMainClasses.join(' ') %>"
        >
          <div
            class="<%-pageName %>__control-bar <%- pageControlBarClasses.join(' ') %>"
          ></div>
          <div
            class="<%-pageName %>-grid mb-sm <%- pageGridClasses.join(' ') %>"
          >
            <div
              class="<%- pageName %>-grid__item <%- pageGridItemClasses.join(' ') %>"
            ></div>
          </div>
          <div class="<%- promptName %>-prompt <%- pagePromptClasses.join(' ') %>""><%= prompt %></div>
        </div>
      </div>
    </main>
  </body>
  <%- include('partials/footer', {year}); %>
</html>
